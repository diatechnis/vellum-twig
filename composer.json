{
    "name": "diatechnis/vellum-twig",
    "description": "Code for using Twig with Vellum.",
    "license": "MIT",
    "authors": [
        {
          "name": "Michael Kelly",
          "email": "puritandesigns@gmail.com"
        }
    ],
    "type": "library",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "twig/twig": "^2.0.0"
    },
    "require-dev": {
        "diatechnis/vellum": "^0.6.1",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "VellumTwig\\": "src/VellumTwig/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/Tests/"
        }
    }
}
